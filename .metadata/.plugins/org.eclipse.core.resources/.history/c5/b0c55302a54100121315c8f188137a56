package genericsTextParser;

import java.util.ArrayList;
import java.util.Collections;
import java.util.List;
import java.util.Scanner;
import java.io.*;

public class HW4 {
	/**
	 * @param args
	 */
	
	
	@SuppressWarnings("resource")
	public static void main(String[] args) throws IOException {
		 FileWriter outFile = new FileWriter("/Users/Eric/Documents/sorted_test.txt");
		 BufferedWriter out = new BufferedWriter(outFile);
		 
		 Scanner inFile = new Scanner(new File("/Users/Eric/Documents/test.txt"));
		 List<String> names = new ArrayList<String>(); {
			 while (inFile.hasNextLine()){
			      String currentLine = inFile.nextLine();
			      names.add(currentLine);
			      System.out.println(currentLine);
			}
		 }
		 
		 Collections.sort(names);
		 
		 System.out.println(names);
		 
		 for(int i = 0; i < names.size(); i++) { 
			for(int j = 0; j < names.size(); j++){
					if (names.get(i).equals(names.get(j) + 1)) {
						names.remove(j + 1);
					} 
				}
			}	
		 
		 for(int x = 0; x < names.size(); x++) {
			 out.write(names.get(x));
			 out.write("\n");
		 }
	}
}


