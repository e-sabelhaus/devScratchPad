package gui;

import javax.swing.*;
import java.awt.event.*;

/**
 * @author Eric
 *
 */
public class WelcomeGUI extends JFrame {
	//create text field
	JTextField a = new JTextField("First Name");
	JTextField b = new JTextField("Last Name");
	JTextField c = new JTextField("Age");
	JTextField d = new JTextField(a + "/t" + b + "/t" + c);
	//create button
	JButton submit = new JButton("Submit");
	//create frames to hold data
	JPanel panel = new JPanel();
	
	panel.add(a);
	panel.add(b);
	panel.add(c);
	panel.add(d);
	panel.add(submit);
	
	add(panel);
	/**
	 * @param args
	 */
	public static void main(String[] args) {
		// TODO Auto-generated method stub

	}

}
