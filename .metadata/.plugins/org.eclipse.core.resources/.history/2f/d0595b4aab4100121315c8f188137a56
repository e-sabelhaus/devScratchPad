package genericsTextParser;

//import dependencies
import java.util.ArrayList;
import java.util.Collections;
import java.util.Scanner;
import java.io.*;

public class HW4 {
	/**
	 * 
	 */
	
	
	public static void main(String[] args) throws IOException {
		/**
		 * 
		 */
		FileWriter outFile = new FileWriter("/Users/Eric/Documents/sorted_test.txt");
		PrintWriter out = new PrintWriter(outFile);
		 
		Scanner inFile = new Scanner(new File("/Users/Eric/Documents/test.txt"));
		ArrayList<String> names = new ArrayList<String>(); {
			while (inFile.hasNextLine()){
				String currentLine = inFile.nextLine();
			    names.add(currentLine);
			}
		}
		 
		Collections.sort(names);
		 
		int size = names.size();
		for(int i = 0; i < size; i++) { 
			for (int j = i + 1; j < size; j++) {
				if (names.get(j).equals(names.get(i))) {
					names.remove(j);
					j--;
					size--;
				}
				
			}
		}
		 
		for(int x = 0; x < names.size(); x++) {
			out.println(names.get(x));
		}
		out.close();
		inFile.close();
	}
}


