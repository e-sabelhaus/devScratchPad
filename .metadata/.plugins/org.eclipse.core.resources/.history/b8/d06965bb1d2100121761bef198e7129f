package car;

import javax.swing.JOptionPane;

public class CarTest {
	
	public static void main(String[] args) {
		
		Car car1 = new Car(make, model, year, price);
		
		String newMake = JOptionPane.showInputDialog("Input Make");
		
		String newModel = JOptionPane.showInputDialog("Input Model");
		
		String newYear = JOptionPane.showInputDialog("Input Year");
		
		String newPrice = JOptionPane.showInputDialog("Input Price");
		
		car1.getMake();
		
		car1.getModel();
		
		car1.getYear();
		
		car1.getPrice();
		
		car1.setMake(newMake);
		
		car1.setModel(newModel);
		
		car1.setYear(newYear);
		
		car1.setPrice(newPrice);
		
		car1.toString();
		
		System.out.println(car1);
	}

}
