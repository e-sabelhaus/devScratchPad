		photo = newPhoto;
	}
	
	public FileInputStream setFile(String photo) throws FileNotFoundException {
	File file = new File(photo);
	
	FileInputStream photoFile = new FileInputStream(file);
	
	return photoFile;
	}
	PixelDecryptor(FileInputStream photoFile) throws IOException {
		Picture picture = new Picture(photoFile);
	}
	
	public Color decryptPixel(Color pixel) {
		return pixel;
		
	}
	public int getPixelCount() {
		return 0;
		
	}
	public int[] getMaxColorValues() {
		return null;
		
	}
}
