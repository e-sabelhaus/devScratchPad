package shuffleCards;

public class dealCards {
 
    public static void main(String[] args) {

    	Deck deck1 = new Deck();	

    	Deck deck2 = new Deck();

	deck1.deal();

	deck2.deal();
	
	for(int i = 0; i < 51; i++) {
		Card card1 = deck1.getCard(i);
		
		Card card2 = deck2.getCard(i);
		
		System.out.println(card1 + "\t" + card2);
	}
	
    }

}
